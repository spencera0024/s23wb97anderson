extends layout

block content
  h1= title
  - var image_names = ["gem image 1.webp", "gem image 2.jpg", "gem image 3.png", "gem image 4.jpg", "gem image 5.jpg"]
  - for(let i = 0; i < 3; i++) {
        - let randomNum = Math.floor(Math.random()*5)
        img(src=`/images/${image_names[randomNum]}`, height=300, width=300, alt=`${image_names[randomNum]}`)
  - }